{
  "name": "@ahlec/s3-deployer",
  "version": "1.0.1",
  "description": "Node CLI tool for customized, powerful deploys to S3.",
  "author": "Alec Jacob Deitloff <alec.deitloff@gmail.com>",
  "type": "module",
  "keywords": [
    "s3",
    "deploy",
    "upload",
    "cloudfront",
    "aws",
    "publish"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bin": "./lib/cli/cli.js",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "ts-node ./src/cli/cli.ts",
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "prettier --write ."
  },
  "files": [
    "lib/**/*.js",
    "lib/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ahlec/s3-deployer.git"
  },
  "bugs": {
    "url": "https://github.com/ahlec/s3-deployer/issues"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.43.0",
    "@aws-sdk/client-s3": "^3.44.0",
    "ajv": "^8.9.0",
    "chalk": "4.1.2",
    "cosmiconfig": "^8.3.6",
    "inquirer": "^8.2.0",
    "md5": "^2.3.0",
    "micromatch": "^4.0.4",
    "mime-types": "^2.1.34",
    "wrap-ansi": "^7.0.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@types/md5": "^2.3.1",
    "@types/micromatch": "^4.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.6.3",
    "@types/wrap-ansi": "^8.0.1",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "prettier": {},
  "packageManager": "yarn@3.6.3"
}
